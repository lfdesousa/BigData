#below another very simple example of how we can add some data to the stream
# that we have previously created
aws kinesis put-record --stream-name test-stream --partition-key 1 --data 1
aws kinesis put-record --stream-name test-stream --partition-key 2 --data 2
aws kinesis put-record --stream-name test-stream --partition-key 3 --data 3
aws kinesis put-record --stream-name test-stream --partition-key 4 --data 4
